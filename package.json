{
	"name": "intermediate-react",
	"version": "1.0.0",
	"description": "learning react via frontend masters",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"build:client": "vite build --outDir ../dist/client",
		"build:server": "vite build --outDir ../dist/server --ssr Server.jsx",
		"build": "npm run build:client && npm run build:server",
		"dev": "vite --host",
		"format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
		"preview": "vite preview",
		"start": "node server.js"
	},
	"keywords": [],
	"author": "displaycoffee",
	"license": "ISC",
	"devDependencies": {
		"@vitejs/plugin-react": "^4.1.0",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.28.1",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"prettier": "^3.0.3",
		"vite": "^4.4.11"
	},
	"dependencies": {
		"@tanstack/react-query": "^4.35.3",
		"express": "^4.18.2",
		"react": "^18.2.0",
		"react-cookie": "^6.1.1",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.16.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/displaycoffee/intermediate-react.git"
	},
	"bugs": {
		"url": "https://github.com/displaycoffee/intermediate-react/issues"
	},
	"homepage": "https://github.com/displaycoffee/intermediate-react#readme",
	"browserslist": [
		"last 2 versions"
	]
}
